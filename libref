pdirectory=ParentDirectory
aiinaaldir=ParentDirectory/AIInAAL
export LD_LIBRARY_PATH=ParentDirectory/AIInAAL/AIInAAL_env/lib
export USE_XETLA=OFF
export SYCL_PI_LEVEL_ZERO_USE_IMMEDIATE_COMMANDLISTS=1
export SYCL_CACHE_PERSISTENT=1
source ParentDirectory/AIInAAL/AIInAAL_env/bin/ipex-llm-init -g --device Arc

AIInAAL_update () {
    cd ParentDirectory/AIInAAL
    git switch main
    git reset --hard HEAD
    git pull
}